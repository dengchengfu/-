Private Sub Worksheet_Change(ByVal Target As Range)

    For Each MR In Target
        pd_name = MR.Value
        If pd_name = "羽缎粉饼" Then
            tg_row = MR.Row
            tg_column = MR.Column
            Worksheets("Sheet1").Cells(tg_row, tg_column) = "羽毛粉饼"
        End If
    Next
    
End Sub
